{
  "extends": "airbnb",
  "rules": {
    "indent": [2, 4],
    "valid-jsdoc": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "padded-blocks": 0,
    "no-var": 0,
    "comma-dangle": 0,
    "strict": 0,
    "vars-on-top": 0,
    "no-use-before-define": 0,
    "id-length": 0,
    "no-param-reassign": 0
  },
  "globals": {
    "it": false,
    "before": false,
    "after": false,
    "beforeEach": false,
    "afterEach": false,
    "describe": false,
    "chrome": false,
    "assert": false,
    "sinon": false
  }
}